	CONSUME r1,s1;
	PRODUCE r2,s2;

TRANSITION c2
	CONSUME r2,s2;
	PRODUCE r3,s3;

TRANSITION c3
	CONSUME r3,s3;
	PRODUCE r4,s4;

TRANSITION d1
	CONSUME p1;
	PRODUCE p2;

TRANSITION d2
	CONSUME p2;
	PRODUCE p3;

TRANSITION d3
	CONSUME p3;
	PRODUCE p4,y1;

TRANSITION d4
	CONSUME p4;
	PRODUCE q1;

TRANSITION e1
	CONSUME q1;
	PRODUCE q2;

TRANSITION e2
	CONSUME q2;
	PRODUCE q3;

TRANSITION e3
	CONSUME q3;
	PRODUCE q4,y1;

TRANSITION e4
