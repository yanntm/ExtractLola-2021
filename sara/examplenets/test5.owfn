	PRODUCE p2,q2;

TRANSITION a2
	CONSUME p2,q2,y1;
	PRODUCE p3,q3;

TRANSITION a3
	CONSUME p3,q3;
	PRODUCE p4,q4;

TRANSITION b1
	CONSUME q1,r1;
	PRODUCE q2,r2;

TRANSITION b2
	CONSUME q2,r2;
	PRODUCE q3,r3;

TRANSITION b3
	CONSUME q3,r3;
	PRODUCE q4,r4;

