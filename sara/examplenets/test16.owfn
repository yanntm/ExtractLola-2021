
PLACE
  INTERNAL
    s, p1, p2, q1, q2, f;

INITIALMARKING
    s:3;

FINALMARKING
	f:3;

TRANSITION e1
	CONSUME s;
	PRODUCE p1;

TRANSITION e2
	CONSUME s;
	PRODUCE p2;

TRANSITION i1
	CONSUME p1,p2;
	PRODUCE q1,p2;

TRANSITION i2
	CONSUME p1,p2;
	PRODUCE p1,q2;

TRANSITION x1
	CONSUME q1;
	PRODUCE f;

TRANSITION x2
	CONSUME q2;
	PRODUCE f;

